Options All -Indexes
ErrorDocument 404 /404/
ErrorDocument 403 /403/
<IfModule mod_rewrite.c>
RewriteEngine on
RewriteBase /
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*) index.php?page=$1 [L,QSA]
</IfModule>
<FilesMatch ".(htaccess|json|nyan)$">
 Order Allow,Deny
 Deny from all
 </FilesMatch>
